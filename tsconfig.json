{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "baseUrl": "src",
    "jsx": "react",
    "target": "es6",
    "module": "ESNext",
    "moduleResolution": "node",
    "paths": {
      "@red/*": ["red/*"],
      "@yellow/*": ["yellow/*"],
      "@green/*": ["green/*"],
      "@blue/*": ["blue/*"]
    }
  }
}
